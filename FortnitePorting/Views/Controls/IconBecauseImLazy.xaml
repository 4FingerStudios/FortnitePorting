<UserControl x:Class="FortnitePorting.Views.Controls.IconBecauseImLazy"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FortnitePorting.Views.Controls">
    <Image Source="{Binding ImageSource, RelativeSource={RelativeSource AncestorType=local:IconBecauseImLazy}}" Width="{Binding IconSize, RelativeSource={RelativeSource AncestorType=local:IconBecauseImLazy}}" Height="{Binding IconSize, RelativeSource={RelativeSource AncestorType=local:IconBecauseImLazy}}" RenderOptions.BitmapScalingMode="Fant" />
</UserControl>